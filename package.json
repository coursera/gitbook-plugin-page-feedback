{
    "name": "gitbook-plugin-page-feedback",
    "description": "Page feedback for gitbook",
    "main": "index.js",
    "version": "0.1.10",
    "engines": {
        "gitbook": ">=2.5.1"
    },
    "homepage": "https://github.com/coursera/gitbook-plugin-page-feedback",
    "repository": {
        "type": "git",
        "url": "https://github.com/coursera/gitbook-plugin-page-feedback"
    },
    "license": "Apache",
    "bugs": {
        "url": "https://github.com/coursera/gitbook-plugin-page-feedback/issues"
    },
    "gitbook": {
        "properties": {
            "google-script-web-deploy-endpoint": {
                "type": "string",
                "title": "Webhook url for Google script",
                "required": true
            },
            "slack-channel": {
                "type": "string",
                "title": "Channel name for Slack",
                "required": false
            },
            "slack-webhook": {
                "type": "string",
                "title": "Webhook url for Slack",
                "required": false
            }
        }
    }
}
